{
  "name": "nodereactstart",
  "version": "1.0.0",
  "description": "My Full Stack React Node Starter",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "node",
    "react",
    "express"
  ],
  "author": "simon lee",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.3"
  }
}
