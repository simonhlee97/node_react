    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "joi": "^13.2.0",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.3",
    "pug": "^2.0.3",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "validator": "^9.4.1"
  }
}
